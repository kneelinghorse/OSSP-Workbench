{
  "name": "ossp-consumer-gen",
  "version": "1.0.0",
  "type": "module",
  "description": "Protocol-Driven Discovery tooling and missions",
  "files": [
    "packages/",
    "templates/",
    "cli/",
    "src/",
    "utils/",
    "config/",
    "viewers/",
    "README.md",
    "CHANGELOG.md"
  ],
  "bin": {
    "ossp-consumer-gen": "./packages/runtime/cli/generate-cli.js",
    "ossp": "./packages/runtime/cli/index.js",
    "ossp-mcp-server": "./packages/runtime/bin/protocol-mcp-server.js",
    "protocol-discover": "./packages/runtime/cli/index.js",
    "protocol-generate": "./packages/runtime/cli/generate-cli.js",
    "protocol-mcp-server": "./packages/runtime/bin/protocol-mcp-server.js"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --maxWorkers=2 --testTimeout=30000",
    "test:fast": "jest --config ./jest.fast.config.js --ci --maxWorkers=2 --testTimeout=20000",
    "test:packaging": "jest --runTestsByPath tests/distribution/npm-package.test.ts --ci --runInBand",
    "test:golden:compat": "jest --runTestsByPath tests/generators/consumer-golden-compatibility.test.js --ci --runInBand",
    "test:golden:update": "node scripts/validation/update-golden-consumers.js",
    "test:integration:brokers": "RUN_BROKER_INTEGRATION=1 jest --runTestsByPath tests/integration/broker-testcontainers.integration.test.ts --ci --runInBand --testTimeout=300000",
    "generate:sprint-summary": "tsx ./scripts/reports/sprint-summary-generator.ts",
    "test:esm:probe": "jest -c ./jest.esm.ci.js --runTestsByPath tests/_probe/esm.sample.test.ts",
    "test:performance": "node packages/runtime/scripts/performance-benchmark.js",
    "test:deflake": "node tests/util/deflake-runner.js",
    "test:deflake:report": "node tests/util/deflake-reporter.js",
    "check:boundaries": "node scripts/check-layer-boundaries.js",
    "demo:openapi-viz": "node ./examples/demos/openapi-to-viz/run-demo.js",
    "demo:asyncapi-consumer": "node ./examples/demos/asyncapi-to-consumer/run-demo.js",
    "cli": "node ./packages/runtime/cli/index.js",
    "build": "echo 'No build step required for Node.js'",
    "start": "node packages/runtime/bin/protocol-mcp-server.js",
    "docker:build": "docker build -t ossp-agi-mcp:latest .",
    "docker:run": "docker run -d --name ossp-agi-mcp -e PROTOCOL_ROOT=/app ossp-agi-mcp:latest",
    "docker:stop": "docker stop ossp-agi-mcp && docker rm ossp-agi-mcp",
    "ci:full": "npm run check:boundaries && npm run test:ci && npm run test:performance && npm run docker:build",
    "probe:pre12": "node scripts/probe/pre12.collect.js all",
    "test:e2e:mcp": "jest --runTestsByPath tests/e2e/mcp.e2e.test.ts",
    "validate:asyncapi:realworld": "node scripts/validation/real-world-asyncapi-validation.js",
    "validate:soak:performance": "node scripts/validation/consumer-soak-performance.js",
    "check:soak:budget": "node scripts/ci/consumer-soak-budget.js",
    "ci:nightly:soak": "npm run validate:soak:performance && npm run check:soak:budget",
    "release:sbom": "node scripts/release/generate-sbom.js",
    "release:gate": "npm run test:packaging && npm run test:golden:compat && npm run validate:asyncapi:realworld && SOAK_ITERATIONS=3 npm run validate:soak:performance && npm run check:soak:budget && npm audit --omit=dev --audit-level=high && npm run release:sbom && npm pack --dry-run"
  },
  "devDependencies": {
    "@apidevtools/swagger-parser": "^10.0.3",
    "@asyncapi/parser": "^2.1.2",
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-typescript": "^7.27.1",
    "@kafkajs/confluent-schema-registry": "^3.9.0",
    "@testcontainers/kafka": "^11.11.0",
    "@testcontainers/rabbitmq": "^11.11.0",
    "amqplib": "^0.10.9",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "kafkajs": "^2.2.4",
    "light-my-request": "^6.6.0",
    "mqtt": "^5.15.0",
    "supertest": "^7.1.4",
    "testcontainers": "^11.11.0",
    "tsx": "^4.15.7",
    "yaml": "^2.8.1"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "chalk": "^5.6.2",
    "cli-table3": "^0.6.5",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "eventemitter3": "^5.0.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "fs-extra": "^11.3.2",
    "graphology": "^0.26.0",
    "graphology-traversal": "^0.3.1",
    "handlebars": "^4.7.8",
    "inquirer": "^12.9.6",
    "lru-cache": "^10.4.3",
    "node-fetch": "^3.3.0",
    "ora": "^7.0.1",
    "p-retry": "^5.1.2",
    "pg": "^8.16.3",
    "xmlbuilder2": "^3.1.1",
    "xxhash-addon": "^2.0.3"
  }
}
